NAME _PROGRAM ; 
BODY DATA INTEGER16 _AAAAAAA , _BBBBBBB , _XXXXXXX , _YYYYYYY ;
  GET ( _AAAAAAA )
  GET ( _BBBBBBB )
  
  PUT ( _AAAAAAA + _BBBBBBB )
  PUT ( _AAAAAAA - _BBBBBBB )
  PUT ( _AAAAAAA * _BBBBBBB )
  PUT ( _AAAAAAA DIV _BBBBBBB )
  PUT ( _AAAAAAA MOD _BBBBBBB )

  _XXXXXXX := (_AAAAAAA - _BBBBBBB) * 10 + (_AAAAAAA + _BBBBBBB) DIV 10
  _YYYYYYY := _XXXXXXX + _XXXXXXX MOD 10

  PUT ( _XXXXXXX )
  PUT ( _YYYYYYY )

END