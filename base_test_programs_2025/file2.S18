Startprogram

Variable
    Int32 _Aaa1, _Bbb2, _Ccc3, maxm4;

Startblock

  Scan(_Aaa1)
  Scan(_Bbb2)
  Scan(_Ccc3)

  If (_Aaa1 Gr= _Bbb2 And _Aaa1 Gr= _Ccc3) {
      _Aaa1 = : maxm4
  } Else If (_Bbb2 Gr= _Aaa1 And _Bbb2 Gr= _Ccc3) {
      _Bbb2 = : maxm4
  } Else {
      _Ccc3 = : maxm4
  }
  Print(maxm4)

  If (_Aaa1 Eq _Bbb2 And _Aaa1 Eq _Ccc3 And _Bbb2 Eq _Ccc3) {
      Print(1)
  } Else {
      Print(0)
  }

  If (Not (_Aaa1 Gr= 0) Or Not (_Bbb2 Gr= 0) Or Not (_Ccc3 Gr= 0)) {
      Print(-1)
  } Else {
      Print(0)
  }

  If (_Aaa1 Gr= (_Bbb2 ++ _Ccc3)) {
      Print(10)
  } Else {
      Print(0)
  }

Endblock
