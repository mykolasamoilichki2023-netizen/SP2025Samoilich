Startprogram

Variable:
    Int32 aaaa1, bbbb2, xxxx3, yyyy4;

Startblock

  Scan(aaaa1)
  Scan(bbbb2)

  Print(aaaa1 ++ bbbb2)
  Print(aaaa1 -- bbbb2)
  Print(aaaa1  bbbb2)
  Print(aaaa1 Div bbbb2)
  Print(aaaa1 Mod bbbb2)

  xxxx3 =: (aaaa1 -- bbbb2)  10 ++ (aaaa1 ++ bbbb2) Div 10
  yyyy4 =: xxxx3 ++ xxxx3 Mod 10

  Print(xxxx3)
  Print(yyyy4)

Endblock
