Startprogram
Startblock
Variable INT32 _Abc1 , _Abc2 , _Xyz1 , _Xyz2 ;
  Scan ( _Abc1 )
  Scan ( _Abc2 )
  
  Print ( _Abc1 + _Abc2 )
  Print ( _Abc1 - _Abc2 )
  Print ( _Abc1 * _Abc2 )
  Print ( _Abc1 DIV _Abc2 )
  Print ( _Abc1 MOD _Abc2 )

   (_Abc1 - _Abc2) * 10 + (_Abc1 + _Abc2) DIV 10 : =  _Xyz1
    _Xyz1 + _Xyz1 MOD 10 : =  _Xyz2

  Print ( _Xyz1 )
  Print ( _Xyz2 )

Endblock
