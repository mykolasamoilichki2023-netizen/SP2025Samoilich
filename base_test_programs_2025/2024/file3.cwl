NAME _PROGRAM ; 
BODY DATA INTEGER16 _AAAAAAA , _BBBBBBB , _CYCLEAA , _CYCLEBB , _XVALUEE;
  GET ( _AAAAAAA )
  GET ( _BBBBBBB )

 FOR _CYCLEAA << _AAAAAAA TO _BBBBBBB DO
    PUT ( _CYCLEAA * _CYCLEAA )
 ;

  _XVALUEE << 0
  FOR _CYCLEAA << 1 TO _AAAAAAA DO
    FOR _CYCLEBB << 1 TO _BBBBBBB DO
      _XVALUEE << _XVALUEE + 1
    ;
  ;
  PUT ( _XVALUEE )

END